package primerosPasos;
import javax.swing.*;

public class Uso_arrays_I {

	public static void main(String[] args) {
		
		String paises[]= new String[8];
		
		for(int i=0; i<8; i++) {
			paises[i]=JOptionPane.showInputDialog("Introduce país " + (i+1));
		}
		
		System.out.println("País " + paises);
		

	}

}
